      "import": "./index.js",
      "default": "./index.js"
    }
  },
  "license": "ISC",
  "keywords": [
    "next-auth",
    "next.js",
    "auth.js",
    "redis",
    "cache"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@auth/core": "workspace:*"
  },
  "peerDependencies": {
    "@redis/client": "^1.6.0"
  },
  "devDependencies": {
    "@redis/client": "^1.6.0",
    "typescript": "^5.7.3"
  }
}
